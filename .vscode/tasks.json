{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++: gcc アクティブなファイルのビルド",
      "command": "/usr/bin/gcc",
      "args": [
        "-fdiagnostics-color=always",
        "-g",
        "${file}",
        "${fileDirname}/operand.c",
        "${fileDirname}/stack.c",
        "${fileDirname}/util.c",
        "-o",
        "main"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "デバッガーによって生成されたタスク。"
    }
  ],
  "version": "2.0.0"
}
